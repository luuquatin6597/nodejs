<!-- Breadcrumb Section Begin -->
<div class="breacrumb-section">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="breadcrumb-text">
                    <a href="/"><i class="fa fa-home"></i> Home</a>
                    <span>Shop</span>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Breadcrumb Section Begin -->

<!-- Product Shop Section Begin -->
<section class="product-shop spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 order-1 order-lg-2">
                <div class="product-list">
                    <div class="row">
                        <% const products = [
                            { id: 1, name: 'Pure Pineapple', category: 'Towel', price: 14, isSale: true },
                            { id: 2, name: 'Guangzhou sweater', category: 'Coat', price: 13, isSale: false },
                            { id: 3, name: 'Guangzhou sweater', category: 'Shoes', price: 34, isSale: false },
                            { id: 4, name: 'Microfiber Wool Scarf', category: 'Coat', price: 64, isSale: false },
                            { id: 5, name: "Men's Painted Hat", category: 'Shoes', price: 44, isSale: false },
                            { id: 6, name: 'Converse Shoes', category: 'Shoes', price: 34, isSale: false },
                            { id: 7, name: 'Pure Pineapple', category: 'Towel', price: 64, isSale: true },
                            { id: 8, name: '2 Layer Windbreaker', category: 'Coat', price: 44, isSale: false },
                            { id: 9, name: 'Converse Shoes', category: 'Shoes', price: 34, isSale: false }
                        ] %>
                        <% products.forEach(product => { %>
                        <div class="col-lg-4 col-sm-6">
                            <%- include('../components/product-item', { product }) %>
                        </div>
                        <% }) %>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- Product Shop Section End -->

<!-- Partner Logo Section Begin -->
<div class="partner-logo">
    <div class="container">
        <div class="logo-carousel owl-carousel">
            <% for(let i = 1; i <= 5; i++) { %>
            <div class="logo-item">
                <div class="tablecell-inner">
                    <img src="/img/logo-carousel/logo-<%= i %>.png" alt="">
                </div>
            </div>
            <% } %>
        </div>
    </div>
</div>
<!-- Partner Logo Section End --> 